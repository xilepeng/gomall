{{template "header" .}}

<div class="row">
  {{range .Items}}
  <div class="card col-12 col-sm-8 col-md-6 col-lg-4  col-xl-2 p-2 border-light">
    <img src="{{.Picture}}" class="card-img-top" alt="...">
    <div class="card-body">
      <p class="card-text">{{.Name}}</p>
      <h5 class="card-title">{{.Price}}</h5>
    </div>
  </div>

  {{end}}
</div>
{{template "footer"}}